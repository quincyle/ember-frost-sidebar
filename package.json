    "ember-resolver": "^2.0.3",
    "ember-sinon": "~0.6.0",
    "ember-source": "~2.12.0",
    "ember-spread": "^1.2.2",
    "ember-test-utils": "^4.0.0",
    "ember-truth-helpers": "^1.2.0",
