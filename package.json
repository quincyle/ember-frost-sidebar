  },
  "repository": "git@github.com:ciena-frost/ember-frost-sidebar.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "ember-browserify": "~1.1.0",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "~0.3.2",
    "ember-cli-code-coverage": "0.3.5",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-frost-blueprints": "^1.2.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "~0.2.0",
    "ember-cli-mocha": "~0.13.2",
    "ember-cli-notifications": "^4.0.6",
    "ember-cli-shims": "^1.0.2",
